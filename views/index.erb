<% if session[:error] %>
  <p><%= session.delete(:error) %></p>
<% end %>

<% if session[:success] %>
  <p><%= session.delete(:success) %></p>
<% end %>

<ul>
  <% @files.each do |file| %>
    <li>
      <a href="/<%= file %>"><%= file %></a>
      <a href="<%= file %>/edit">Edit</a>
    </li>
  <% end %>
</ul>